<?xml version="1.0" encoding="utf-8"?>

 <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/relativeLayout1"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent" 
        android:background="@drawable/blackdot" >
      
 <ScrollView
     android:id="@+id/scrollview"
     android:layout_width="fill_parent"
     android:layout_height="match_parent" >

   <RelativeLayout
       android:id="@+id/relativeLayout2"
       android:layout_width="match_parent"
       android:layout_height="match_parent" >

   <LinearLayout
       android:id="@+id/linearLayout1"
       android:layout_width="wrap_content"
       android:layout_height="24dp"
       android:layout_alignParentLeft="true"
       android:layout_alignParentRight="true"
       android:layout_alignParentTop="true" 
       android:background="@color/orange">

       <ImageView
           android:id="@+id/imageView1"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:src="@drawable/taxismall" />

       <ImageView
           android:id="@+id/imageView4"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:src="@drawable/officesmall" />

       <TextView
           android:id="@+id/textView1"
           android:layout_width="match_parent"
           android:layout_height="wrap_content"
           android:background="@color/orange"
           android:gravity="center"
           android:height="22dp"
           android:text="Delhi Taxi And Offices"
           android:textColor="@color/Black"
           android:textSize="16dp"
           android:textStyle="bold"
           android:typeface="serif" />

   </LinearLayout>

   <TextView
       android:id="@+id/textView2"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_alignParentLeft="true"
       android:layout_alignParentRight="true"
       android:layout_below="@+id/linearLayout1"
       android:background="@color/Black"
       android:height="2dp"
        />


   <LinearLayout
       android:id="@+id/linearLayout2"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_alignParentLeft="true"
       android:layout_alignParentRight="true"
       android:layout_below="@+id/textView2" >

       <ImageView
           android:id="@+id/imageView2"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:src="@drawable/officeslarge" />

       <TextView
           android:id="@+id/textView3"
           android:layout_width="wrap_content"
           android:layout_height="match_parent"
           android:text="     " />

       <Button
           android:id="@+id/UMSbuttondelhiofficies"
           android:layout_width="match_parent"
           android:layout_height="match_parent"
           android:layout_gravity="center"
           android:layout_weight="1"
           android:gravity="center"
           android:text="Delhi Offices"
           android:textColor="@color/Marom"
           android:textSize="19dp"
           android:textStyle="bold"
           android:typeface="serif" />

   </LinearLayout>

   <TextView
       android:id="@+id/textView4"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_alignParentLeft="true"
       android:layout_alignParentRight="true"
       android:layout_below="@+id/linearLayout2"
       android:height="6dp" />

   <TextView
       android:id="@+id/textView5"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_alignParentLeft="true"
       android:layout_alignParentRight="true"
       android:layout_below="@+id/textView4"
       android:background="@color/orange"
       android:height="2dp" />

   <TextView
       android:id="@+id/textView6"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_alignParentLeft="true"
       android:layout_alignParentRight="true"
       android:layout_below="@+id/textView5"
       android:height="6dp"/>

   <LinearLayout
       android:id="@+id/linearLayout3"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_alignParentLeft="true"
       android:layout_alignParentRight="true"
       android:layout_below="@+id/textView6" >

       <ImageView
           android:id="@+id/imageView3"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:src="@drawable/taxilarge" />

       <TextView
           android:id="@+id/textView7"
           android:layout_width="wrap_content"
           android:layout_height="match_parent"
           android:text="     " />

       <Button
           android:id="@+id/UMSbuttontaxiservices"
           android:layout_width="match_parent"
           android:layout_height="match_parent"
           android:layout_weight="1"
           android:text="Taxi Services"
           android:textColor="@color/Marom"
           android:textSize="19dp"
           android:textStyle="bold"
           android:typeface="serif" />

   </LinearLayout>

   <TextView
       android:id="@+id/textView8"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_alignParentLeft="true"
       android:layout_alignParentRight="true"
       android:layout_below="@+id/linearLayout3"
       android:height="6dp"/>

   <TextView
       android:id="@+id/textView9"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_alignParentLeft="true"
       android:layout_alignParentRight="true"
       android:layout_below="@+id/textView8"
       android:background="@color/orange"
       android:height="2dp"/>

   <TextView
       android:id="@+id/textView10"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_alignParentLeft="true"
       android:layout_alignParentRight="true"
       android:layout_below="@+id/textView9"
       android:height="6dp" />

   <LinearLayout
       android:id="@+id/linearLayout4"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_alignParentLeft="true"
       android:layout_alignParentRight="true"
       android:layout_below="@+id/textView10" >

       <TextView
           android:id="@+id/textView11"
           android:layout_width="wrap_content"
           android:layout_height="match_parent"
           android:text="Delhi Offices : "
           android:textColor="@color/orange"
           android:textSize="15dp"
           android:textStyle="bold"
           android:typeface="serif" 
           android:gravity="center"
           />

       <TextView
           android:id="@+id/textView12"
           android:layout_width="match_parent"
           android:layout_height="wrap_content"
           android:text="Display Info about Major Officies in Delhi like Embassy, Tourism, Security, transport" 
           android:textSize="13dp"
           android:textStyle="italic"
           android:typeface="serif"/>

   </LinearLayout>

   <LinearLayout
       android:id="@+id/linearLayout5"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_alignParentLeft="true"
       android:layout_alignParentRight="true"
       android:layout_below="@+id/linearLayout4" >

       <TextView
           android:id="@+id/textView14"
           android:layout_width="match_parent"
           android:layout_height="wrap_content"
           android:height="6dp" />

   </LinearLayout>

   <TextView
       android:id="@+id/textView15"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_alignParentLeft="true"
       android:layout_alignParentRight="true"
       android:layout_below="@+id/linearLayout5"
       android:background="@color/orange"
       android:height="2dp" />

   <TextView
       android:id="@+id/textView16"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_alignParentLeft="true"
       android:layout_alignParentRight="true"
       android:layout_below="@+id/textView15"
       android:height="6dp"/>

   <LinearLayout
       android:id="@+id/linearLayout6"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_alignParentLeft="true"
       android:layout_alignParentRight="true"
       android:layout_below="@+id/textView16" >

       <TextView
           android:id="@+id/TextView01"
           android:layout_width="wrap_content"
           android:layout_height="match_parent"
           android:gravity="center"
           android:text="Taxi Services : "
           android:textColor="@color/orange"
           android:textSize="15dp"
           android:textStyle="bold"
           android:typeface="serif" />

       <TextView
           android:id="@+id/TextView02"
           android:layout_width="match_parent"
           android:layout_height="wrap_content"
           android:text="Display Info about Taxi Services in Delhi and NCR along with their Fares and servics"
           android:textSize="13dp"
           android:textStyle="italic"
           android:typeface="serif" />

   </LinearLayout>

   <TextView
       android:id="@+id/textView13"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_alignParentLeft="true"
       android:layout_alignParentRight="true"
       android:layout_below="@+id/linearLayout6"
       android:height="6dp" />

   <TextView
       android:id="@+id/textView17"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_alignParentLeft="true"
       android:layout_alignParentRight="true"
       android:layout_below="@+id/textView13"
       android:background="@color/orange"
       android:height="2dp"/>

   <TextView
       android:id="@+id/textView18"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_alignParentLeft="true"
       android:layout_alignParentRight="true"
       android:layout_below="@+id/textView17"
       android:height="6dp" />

   <LinearLayout
       android:id="@+id/linearLayout7"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_alignParentLeft="true"
       android:layout_alignParentRight="true"
       android:layout_below="@+id/textView18" 
       android:gravity="right"
      >

       <ImageView
           android:id="@+id/UMSimageviewback"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:src="@drawable/back" />

   </LinearLayout>

   <TextView
       android:id="@+id/textView19"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_alignParentLeft="true"
       android:layout_alignParentRight="true"
       android:layout_below="@+id/linearLayout7"
       android:height="6dp"/>

   <TextView
       android:id="@+id/textView20"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_alignParentLeft="true"
       android:layout_alignParentRight="true"
       android:layout_below="@+id/textView19"
       android:text="Avoid polythene and make Delhi clean Delhi"
       android:gravity="center"
       android:typeface="serif"
       android:textSize="13dp"
       android:textAppearance="?android:attr/textAppearanceMedium" />

     </RelativeLayout>

</ScrollView>
</RelativeLayout>